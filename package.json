{
  "name": "@xellar/sdk",
  "version": "4.3.3",
  "description": "Xellar Embedded Wallet SDK for Javascript/Typescript",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./react-native": {
      "types": "./dist/react-native/index.d.ts",
      "require": "./dist/react-native/index.js",
      "import": "./dist/react-native/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup-node",
    "test": "jest",
    "test:coverage": "jest --collectCoverage",
    "prepare": "husky",
    "lint": "eslint ./src/**/*.ts --max-warnings=0",
    "lint:fix": "eslint ./src/**/*.ts --fix",
    "checker": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xellar-Protocol/ew-sdk.git"
  },
  "keywords": [
    "wallet",
    "crypto"
  ],
  "author": "Xellar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Xellar-Protocol/ew-sdk/issues"
  },
  "homepage": "https://github.com/Xellar-Protocol/ew-sdk#readme",
  "dependencies": {
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/jest": "^29.5.13",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.6"
  },
  "peerDependencies": {
    "react-native-quick-crypto": "^0.7.6"
  },
  "files": [
    "dist",
    "react-native"
  ]
}
